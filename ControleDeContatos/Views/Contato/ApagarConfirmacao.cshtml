@{
    ViewData["Title"] = "Confirmação apagar o contato";
}

<div class="text-center">
    <h3>Realmente deseja apagar o contato (contato)?</h3>

    <a class="btn btn-danger btn-block" role="button" asp-controller="Contato" asp-action="Index">Cancelar</a>
    <a class="btn btn-primary btn-block" role="button" asp-controller="Contato" asp-action="Apagar">Confirmar</a>

</div>


